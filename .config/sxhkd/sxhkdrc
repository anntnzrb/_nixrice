# [[file:readme.org::*Terminal Emulator][Terminal Emulator:1]]
super + Return
  $TERMINAL
# Terminal Emulator:1 ends here

# [[file:readme.org::*Text Editor][Text Editor:1]]
super + v
  $TERMINAL -e $EDITOR
# Text Editor:1 ends here

# [[file:readme.org::*Text Editor][Text Editor:2]]
super + shift + v
  $VISUAL
# Text Editor:2 ends here

# [[file:readme.org::*Browser][Browser:1]]
super + w
  $BROWSER
# Browser:1 ends here

# [[file:readme.org::*Applicaction Launcher][Applicaction Launcher:1]]
super + d
  dmenu_run -i -p 'summon:'
# Applicaction Launcher:1 ends here

# [[file:readme.org::*File Manager][File Manager:1]]
super + e
  $FILE
# File Manager:1 ends here

# [[file:readme.org::*RSS Feed][RSS Feed:1]]
super + n
  $TERMINAL -e newsboat
# RSS Feed:1 ends here

# [[file:readme.org::*Audio][Audio:1]]
super + p
  $TERMINAL -e pulsemixer
# Audio:1 ends here

# [[file:readme.org::*Audio][Audio:2]]
XF86AudioMute
  pamixer -t && pkill -RTMIN+1 dwmblocks
# Audio:2 ends here

# [[file:readme.org::*Audio][Audio:3]]
XF86Audio{Lower, Raise}Volume
  pamixer -{d 5, i 5} && pkill -RTMIN+1 dwmblocks
# Audio:3 ends here

# [[file:readme.org::*Brightness][Brightness:1]]
XF86MonBrightness{Down, Up}
  xbacklight -{dec 10, inc 10} && pkill -RTMIN+2 dwmblocks
# Brightness:1 ends here

# [[file:readme.org::*System Monitor][System Monitor:1]]
super + shift + Escape
  $TERMINAL -e $SYS_MONITOR
# System Monitor:1 ends here

# [[file:readme.org::*Display Selector Dropdown][Display Selector Dropdown:1]]
super + F3
  displayselect
# Display Selector Dropdown:1 ends here

# [[file:readme.org::*Emoji / Unicode List Dropdown][Emoji / Unicode List Dropdown:1]]
super + grave
  dmenuunicode
# Emoji / Unicode List Dropdown:1 ends here

# [[file:readme.org::*Screenshot Utility Dropdown][Screenshot Utility Dropdown:1]]
Print
  maimpick
# Screenshot Utility Dropdown:1 ends here

# [[file:readme.org::*Edit Config Files Dropdown][Edit Config Files Dropdown:1]]
super + shift + period
  edit-cfgs
# Edit Config Files Dropdown:1 ends here

# [[file:readme.org::*System Options Dropdown][System Options Dropdown:1]]
super + shift + x
  sudo sysfns
# System Options Dropdown:1 ends here

# [[file:readme.org::*Network Tools][Network Tools:1]]
super + F12
  $TERMINAL -e sudo nmtui
# Network Tools:1 ends here

# [[file:readme.org::*Transmission Tools][Transmission Tools:1]]
super + F{6, 7}
 {td-toggle, torwrap}
# Transmission Tools:1 ends here

# [[file:readme.org::*Mouting Tools][Mouting Tools:1]]
super + F{9, 10}
    {dmenumount, dmenuumount}
# Mouting Tools:1 ends here

# [[file:readme.org::*Kill Current Window][Kill Current Window:1]]
alt + F4
  kill -15 $(xdotool getactivewindow getwindowpid)
# Kill Current Window:1 ends here

# [[file:readme.org::*Kill Xorg Session][Kill Xorg Session:1]]
super + control + alt + Escape
  pkill -15 "Xorg"
# Kill Xorg Session:1 ends here
